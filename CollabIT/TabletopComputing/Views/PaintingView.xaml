<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:s="http://schemas.microsoft.com/surface/2008"
             xmlns:data="clr-namespace:TabletopComputing.Data"
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions" xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="TabletopComputing.Views.PaintingView"
             x:Name="This"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="1600">
    <UserControl.Resources>
    	<data:SurfaceInkEditingModeToBoolConverter x:Key="SurfaceInkEditingModeToBoolConverter" />
    	<Storyboard x:Key="CenterGrow" RepeatBehavior="Forever">
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="ellipse">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="2.455"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="ellipse">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="-16"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="ellipse">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="2.455"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="ellipse">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="24"/>
    		</DoubleAnimationUsingKeyFrames>
    		<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ellipse">
    			<EasingColorKeyFrame KeyTime="0:0:1" Value="#FFC59921"/>
    		</ColorAnimationUsingKeyFrames>
    	</Storyboard>
    </UserControl.Resources>
	<UserControl.Triggers>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded"/>
	</UserControl.Triggers>
    <Grid x:Name="LayoutRoot">
    	<VisualStateManager.VisualStateGroups>
    		<VisualStateGroup x:Name="VisualStateGroup">
    			<VisualState x:Name="Enabled"/>
    			<VisualState x:Name="Disabled"/>
    		</VisualStateGroup>
    	</VisualStateManager.VisualStateGroups>
        <s:TagVisualizer x:Name="TagVisualizer" Margin="0,0,0,164" PreviewVisualizationAdded="TagAddedToLoop" PreviewVisualizationRemoved="TagRemovedFromLoop" Panel.ZIndex="-2">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FF3E454C" Offset="0"/>
        			<GradientStop Color="#FF3E454C" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
            <s:TagVisualizer.Definitions>
            	
            	<s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="0"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="1"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="2"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="3"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="4"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="5"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="6"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="4"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="7"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="8"
            		MaxCount="1"  />
                <s:TagVisualizationDefinition Source="Controls/BeatTag.xaml"
            		TagRemovedBehavior="Fade"
            		Value="9"
            		MaxCount="1"  />
                
            </s:TagVisualizer.Definitions>
            <Grid x:Name="TagVisualizerRoot">
                
                <s:ScatterView x:Name="TagVisualizerScatterView"
                               s:TagVisualizer.IsTagVisualizationHost="True"
                               ItemContainerStyle="{DynamicResource EmptyScatterViewItemStyle}"
                               Margin="100" />
            </Grid>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tagvisKick" Content="Kick" HorizontalAlignment="Left" Margin="0,0,0,4" VerticalAlignment="Bottom" Height="160" Width="224" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        	<i:Interaction.Behaviors>
        		<ei:DataStateBehavior TrueState="Enabled" FalseState="Disabled" Binding="{Binding IsEnabled, ElementName=tagvisKick}">
        			<ei:DataStateBehavior.Value>
        				<System:Boolean>False</System:Boolean>
        			</ei:DataStateBehavior.Value>
        		</ei:DataStateBehavior>
        	</i:Interaction.Behaviors>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgSnare" Content="Snare" HorizontalAlignment="Left" Margin="224,0,0,4" VerticalAlignment="Bottom" Height="160" Width="224" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgPiano" Content="Piano" HorizontalAlignment="Left" Margin="440,0,0,4" VerticalAlignment="Bottom" Height="160" Width="224" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgOpenHH" Content="Open HH" Margin="664,0,712,4" VerticalAlignment="Bottom" Height="160" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgClosedHH" Content="Closed HH" Margin="0,0,488,4" VerticalAlignment="Bottom" Height="160" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48" HorizontalAlignment="Right" Width="224">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgLead" Content="Lead" Margin="0,0,264,4" VerticalAlignment="Bottom" Height="160" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48" HorizontalAlignment="Right" Width="224">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <s:TagVisualizer x:Name="tgWNoise" Content="WNoise" Margin="0,0,40,4" VerticalAlignment="Bottom" Height="160" PreviewVisualizationAdded="TagEnter" PreviewVisualizationMoved="TagEnter" Foreground="Black" FontSize="48" HorizontalAlignment="Right" Width="224">
        	<s:TagVisualizer.Background>
        		<RadialGradientBrush>
        			<GradientStop Color="#FFEBEBEB" Offset="0"/>
        			<GradientStop Color="#FFC2C2C2" Offset="1"/>
        		</RadialGradientBrush>
        	</s:TagVisualizer.Background>
        	<s:TagVisualizer.Definitions>
        		<s:TagVisualizationDefinition/>
        	</s:TagVisualizer.Definitions>
        </s:TagVisualizer>
        <Ellipse x:Name="ellipse" Fill="#FF2185C5" Margin="704,272,720,0" Height="176" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Panel.ZIndex="-1">
        	<Ellipse.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
        			<RotateTransform/>
        			<TranslateTransform/>
        		</TransformGroup>
        	</Ellipse.RenderTransform>
        </Ellipse>
        <TextBlock x:Name="tbBeatCounter" Height="104" Margin="776,320,704,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="48" Foreground="White"><Run Language="sv-se" Text="1"/></TextBlock>
       
    </Grid>
</UserControl>
